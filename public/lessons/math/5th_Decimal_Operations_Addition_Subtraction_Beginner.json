{
  "id": "5th_Decimal_Operations_Addition_Subtraction_Beginner",
  "title": "Decimal Operations: Addition and Subtraction",
  "grade": "5th",
  "difficulty": "Beginner",
  "vocab": [
    {
      "term": "decimal",
      "definition": "A number that uses a decimal point to show parts of a whole"
    },
    {
      "term": "decimal point",
      "definition": "The dot that separates the whole number from the fractional part"
    },
    {
      "term": "tenths",
      "definition": "The first place to the right of the decimal point"
    },
    {
      "term": "hundredths",
      "definition": "The second place to the right of the decimal point"
    },
    {
      "term": "place value",
      "definition": "The value of a digit based on its position in a number"
    },
    {
      "term": "thousandths",
      "definition": "The third place to the right of the decimal point"
    },
    {
      "term": "trailing zero",
      "definition": "A zero at the end of a decimal that does not change its value but helps alignment"
    },
    {
      "term": "regrouping",
      "definition": "Rearranging value across place values when adding or subtracting (also called borrowing)"
    }
  ],
  "teachingNotes": "Emphasize aligning decimal points when adding and subtracting. Use place value charts and visual models. Connect decimals to money for real-world applications.",
  "sample": [
