    { "question": "What is 5.00 - 0.75?", "expectedAny": ["4.25", "four and twenty-five hundredths"] },
    { "question": "Add: 0.6 + 0.14", "expectedAny": ["0.74", "seventy-four hundredths"] },
    { "question": "Subtract: 8.1 - 2.06", "expectedAny": ["6.04", "six and four hundredths"] },
