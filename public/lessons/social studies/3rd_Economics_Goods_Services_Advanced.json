    {"word": "services", "kid_friendly_definition": "Work that people do to help others."},
    {"word": "consumer", "kid_friendly_definition": "A person who buys and uses goods and services."},
    {"word": "producer", "kid_friendly_definition": "A person who makes goods or provides services."},
    {"word": "trade", "kid_friendly_definition": "Exchanging goods or services with others."},
    {"word": "bartering", "kid_friendly_definition": "Trading goods or services without using money."},
    {"word": "supply", "kid_friendly_definition": "How much of something is available."},
    {"word": "demand", "kid_friendly_definition": "How much people want something."},
    {"word": "scarcity", "kid_friendly_definition": "When there is not enough of something people want."},
    {"word": "capital", "kid_friendly_definition": "Money or tools used to start or run a business."},
    {"word": "profit", "kid_friendly_definition": "Money left over after paying costs."},
    {"word": "entrepreneur", "kid_friendly_definition": "Someone who starts their own business."}
